{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true,
      "channel": "beta"
    },
    {
      "name": "alpha", 
      "prerelease": true,
      "channel": "alpha"
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {"type": "chore", "release": "patch"}
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "bash docs/scripts/build-setup-assets.sh"
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": false,
        "releasedLabels": false,
        "assets": [
          {
            "path": "artifacts/setup.sh"
          },
          {
            "path": "artifacts/setup.sh.sha256"
          }
        ]
      }
    ]
  ]
}